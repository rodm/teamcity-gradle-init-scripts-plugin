<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                                   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">

    <bean class="com.github.rodm.teamcity.gradle.scripts.server.InitScriptsAdminController"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.InitScriptsActionsController"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.UploadScriptAction"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.DeleteScriptAction"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.InitScriptsBuildFeature"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.InitScriptsProvider"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.GradleInitScriptsPage"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.DefaultGradleScriptsManager"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.InitScriptsUsageAnalyzer"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.InitScriptsSettingsExtension"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.InitScriptsUsagesReport"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.health.ProjectInspector"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.health.MissingInitScriptsHealthReport"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.health.UnusedInitScriptsHealthReport"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.health.InvalidInitScriptsHealthReport"/>
    <bean class="com.github.rodm.teamcity.gradle.scripts.server.health.SameInitScriptsHealthReport"/>
</beans>
